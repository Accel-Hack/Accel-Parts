    "deploy": "npm install && npx tsc && npm publish",
    "test": "jest"
  },
  "keywords": [],
  "author": "kyosuke",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "jest": {
    "moduleFileExtensions": [
